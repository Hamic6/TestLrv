{
    "version": 2,
    "builds": [
      { "src": "server/admin.js", "use": "@vercel/node" },
      { "src": "package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/server/admin.js" },
      { "src": "/(.*)", "dest": "/dist/index.html" }
    ]
  }
  